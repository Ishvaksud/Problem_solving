#include<stdio.h>
int main(){
    int n,a[100],i,sum;
    scanf("%d",&n);
for (i = 0; i <n; i++)
{
scanf("%d",&a[i]);
}
for ( i = 0;i<n; i++)
{
    if (a[i]>=38)
    {
    if (a[i]%5==3||a[i]%5==4)
{
    sum=a[i]+(5-a[i]%5);
  
}
else if (a[i]%5==0||a[i]%5==1||a[i]%5==2||a[i]%5==3)
{
    sum=a[i];
}
    }
    
 
else if (a[i]<38)
{
sum=a[i];
}

printf("%d\n",sum);


}
}
