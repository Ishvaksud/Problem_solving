

#include<iostream>
using namespace std;
int main()
{
    int t,count;
    cin>>t;
    for (int  i = 0; i <t ;i++)
    {
        int n;
        count=0;
        cin>>n;
        int p=n;
        int a;
        while (p!=0)
        {
        
            a=p%10;
            if(a==0)
            {
                count=count+0;
            }
            else if(n%a==0)
            {
                 count++;
                 
              
            }
              
        p=p/10;
        }
        
         cout<<count<<endl;
    }
   
    
}
