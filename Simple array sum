#sum of the array's elements as a single integer. 


#include<stdio.h>
void simpleArraySum()
{
int arr[1000],n,i,sum=0;
   scanf("%d",&n);
   for (i=0;i<n;i++)
   {
      scanf("%d",&arr[i]);
   }
     for(i=0;i<n;i++)
     {
        sum=sum+arr[i];
     }
     printf("%d",sum);
}
int main()
{
   simpleArraySum();


} 
